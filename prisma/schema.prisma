generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payments {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  amount      Float
  date        DateTime
  type        String
}

model Todo {
  id    Int    @id @default(autoincrement())
  title String
}

model Stats {
  id    Int    @id @default(autoincrement())
  name  String
  value Float
}

model Participants {
  id      Int    @id @default(autoincrement())
  name    String
  surname String
  birthday DateTime
}
